{
  "name": "tree-sitter-t32",
  "version": "2.8.1",
  "description": "Lauterbach TRACE32 script language grammar for Tree-sitter",
  "author": "Christoph Sax <c_sax@mailbox.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://codeberg.org/xasc/tree-sitter-t32/"
  },
  "keywords": [
    "lexer",
    "incremental",
    "parsing",
    "tree-sitter",
    "trace32",
    "t32",
    "lauterbach",
    "practice"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "scripts": {
    "build": "tree-sitter generate",
    "build-wasm": "tree-sitter build --wasm",
    "parse": "tree-sitter parse",
    "tags": "tree-sitter tags",
    "test": "tree-sitter test",
    "highlight": "tree-sitter highlight",
    "playground": "tree-sitter playground",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "devDependencies": {
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "^0.22.2"
  },
  "packageManager": "yarn@4.1.1",
  "main": "bindings/node",
  "types": "bindings/node",
  "tree-sitter": [
    {
      "scope": "source.t32",
      "file-types": [
        "cmm",
        "t32"
      ],
      "injection-regex": "t32",
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm",
      "tags": "queries/tags.scm"
    }
  ],
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp": "^10.0.1",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  }
}
